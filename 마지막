#include <stdio.h>

char result;

printf("%s 님의 퍼스널 컬러는 %s입니다. \n", name, result);

switch (result)
{
case (result==spring warm light):
	printf("%s 님께 어울리는 색상은 다음과 같습니다. \n", name);
	printf("봄 웜 라이트 톤의 컬러는 페일톤과 라이트 톤으로 가볍고 순한 느낌을 줍니다. 아이보리, 라이트 웜 베이지, 라이트 클리어 네이비, 라이트 클리어 골드, 살구색, 코랄 핑크 등의 색이 있습니다. \n");
	printf("%s 님께 어울리는 코디는 다음과 같습니다. \n", name);
	printf("옐로베이스의 부드럽고 맑은 색감이 잘 어울려요. 라이트한 컬러를 입었을 떄 생기있는 특유의 느낌이 살아나며 청순한 윤광을 느낄 수 있어요. 흰기가 섞인 고명도의 컬러와 희뿌연 색감의 옷이 좋습니다. 가을스러운 텁텁한 컬러나 인위적인 색감은 건조하고 경직되어 편안해보이지 않으므로 주의하세요. 그리고 블랙&화이트처럼 극단적인 대비감은 소화하기 어려워요. 대비감을 적당히 좁혀 밝게 연출하는 것이 좋고, 액세서리에 적당한 채도 대비로 포인트를 주는 것도 추천드립니다. 올 블랙이나 어두운 컬러만 사용하는 것은 조심해주세요. 탁색, 저명도보다는 흰기 낭낭한 색감을 입었을 때 오히려 얼굴의 부드러운 힘이 살아나는 것을 느낄 수 있어요. \n");
  printf("옷의 소재는 쉬폰처럼 가볍고 흐르는 듯한 소재가 잘 어울려요. 새틴과 깔끔한 무지 원단도 괜찮아요. 몽글몽글한 양털느낌의 플리스 소재도 좋지만 매트한 질감이 답답하게 느껴질 수 있어요. 탁한 컬러나 매트한 질감은 화이트, 페일과 함께 연출해주세요. 벨벳이나 스웨이드, 레더는 무겁고 텁텁하게 느껴져 소화하기 어렵습니다. \n");
	printf("%s 님께 어울리는 메이크업 스타일은 다음과 같습니다. \n", name);
	printf("1. 눈 화장 \n");
  printf("살구나 피치 컬러 섀도우를 연하게 발라주세요. 갈색 아이라이너를 사용하되, 라인보다는 잘 컬링된 속눈썹을 강조하는 것이 좋습니다. 갈색 마스카라는 자연스러운 느낌을 줍니다.");
  printf("2. 치크 화장 \n");
  printf("형광빛의 코랄 또는 흰빛이 도는 핑크 컬러를 선택하세요. 혈색만 주는 정도로 사용하는 것이 좋습니다. \n");
  printf("3. 입술 화장 \n");
  printf("매트보다는 글로시한 제형의 코랄 컬러, 또는 옐로 베이스의 연한 핑크 컬러가 잘 어울립니다.");
  printf("4. 헤어 \n");
  printf("짙은 브라운 계열로 염색하면 성숙한 이미지를 연출할 수 있습니다. 포니테일로 묶은 뒤, 빠져나온 잔머리에 살짝 웨이브를 주면 발랄함을 더할 수 있습니다. \n");
  break;

case (result == spring warm bright):
	printf("%s 님께 어울리는 색상은 다음과 같습니다. \n", name);
	printf("라이트 카멜, 파스텔 열로우 그린, 피치, 브라이트 코랄, 클리어 브라이트 레드, 라이트 웜 아쿠아, 미디어 바이올렛 등의 색이 있습니다. \n");
	printf("%s 님께 어울리는 코디는 다음과 같습니다. \n", name);
	printf("옐로베이스의 선명하고 맑은 색감이 잘 어울려요. 채도 높은 컬러를 아무렇지 않게 소화하며 봄 브라이트 특유의 생동감 있는 빛을 느낄 수 있어요. 따뜻한 고명도, 고체도의 색감이 좋습니다. 노숙한 느낌의 텁텁한 컬러나 차가운 색감은 얼굴에 회기가 돌게 만들 수 있으니 주의하세요. \n");
  printf("힘이 있고 맑은 타입이기 때문에 대비감이 반드시 필요합니다. 상의와 하의의 대비가 아니어도 괜찮아요. 무늬의 대비감이나 포인트 디테일을 사용해보세요. 타고난 빛이 있어 탁색, 저명도보다는 비비드한 색감을 입었을 때 오히려 얼굴의 힘이 살아나는 것을 느낄 수 있습니다. 대비감이 적을수록 칙칙해보이며 건조해지기 때문에 주의해주세요. \n");
  printf("옷의 소재는 레이스나 꽃장식같은 러블리한 포인트를 사용해주세요. 밋밋한 톤온톤 배색이나 자잘자잘한 직조의 트위드는 대비감을 줄이기 때문에, 큰 무늬가 더 잘 어울립니다. 벨벳이나 스웨이드, 레더는 무겁고 답답하게 느껴져 소화하기 어렵습니다. \n");
  printf("%s 님께 어울리는 메이크업 스타일은 다음과 같습니다. \n", name);
	printf("1. 눈 화장 \n");
  printf("진하게 할 경우 답답하고 나이든 느낌을 주기 쉬워요. 아이 메이크업은 최대한 힘을 빼도록 해요. 펄이 들어간 오렌지 컬러의 섀도우를 넓게 발라 포인트를 줍니다. 갈색 아이 라이너를 이용하여 라인을 그려주고, 스머지하여 자연스럽게 표현해 주세요.\n");
  printf("2. 치크 화장 \n");
  printf("볼 중앙에 피치 컬러의 블러셔를 여리 여리하게 발색하여 아이와 립 컬러를 자연스럽게 연결해 줍니다. \n");
  printf("3. 입술 화장 \n");
  printf("광택감이 있고 쨍한 코랄 핑크나 코랄 레드 컬러의 틴트를 입술에 전체적으로 채워 바르면 과일 같은 이미지를 연출할 수 있습니다. \n");
	printf("4. 헤어 \n");
  printf("노란빛이 도는 밝은 브라운 계열의 염색이 잘 어울립니다. 단발 머리에 부드럽게 층을 낸 뒤 굵은 웨이브를 넣으면 생기 있으면서 온화한 느낌을 줍니다. \n");
  break;

case (result == summer cool light):
	printf("%s 님께 어울리는 색상은 다음과 같습니다. \n", name);
	printf("베이비 블루, 라벤더, 파스텔 라일락, 라임라이트 등의 색이 있습니다. \n");
	printf("%s 님께 어울리는 코디는 다음과 같습니다. \n", name);
	printf("안개가 낀 듯한 부드럽고 맑은 색감이 잘 어울리고, 흰색을 입었을 때 깨끗한 특유의 느낌이 살아나며 은은한 광채를 느낄 수 있어요. 형광기와 흰 기가 섞인 색들도 베스트입니다. 흰색을 벨트, 가방, 신발 등 곳곳에 사용해주세요. 미색이나 로즈베이지 컬러도 괜찮습니다.\n");
  printf("블랙&화이트처럼 큰 대비감은 소화하기 어려워요. 인위적인 느낌 자체를 소화하지 못하기 때문에 블랙보다는 부드러운 먹색이나 차콜 같은 컬러를 추천합니다. 맑은 네이비와 밀크 화이트로 대비감을 살짝 줄여서 연출하면 잘 어울린답니다. 너무 힘 빠진 배색이나 큰 대비감은 안됩니다. 중간 정도의 대비감을 주어야 합니다. \n");
  printf("옷의 소재는 광택이 심하지 않은 실크, 쉬폰 소재가 잘 어울려요. 평평한 민무늬의 면, 공단도 괜찮습니다. 에스닉한 무드의 재질, 패턴, 스타일은 모두 피해주세요. 러블리한 레이스나 볼륨감있는 니트처럼 질감이 도드라지는 옷은 고급스러운 에너지를 분산시켜요. 트위드의 경우 재질감이 약한 어두운 컬러로 골라주세요. 색감이 트위드의 재질감을 잡아 조금 더 잘 소화할 수 있답니다. 액세서리는 골드보다 실버를 선택하는 것이 좋습니다.
	printf("%s 님께 어울리는 메이크업 스타일은 다음과 같습니다. \n", name);
	printf("1. 눈 화장 \n");
  printf("흰빛이 많이 돌고 쉬머한 필이 든 핑크 컬러 셰도우를 연하게 발라주세요. 그리고 블랙 컬러의 아이라이너로 라인을 또렷하고 얇게 그려주세요. \n");
  printf("2. 치크 화장 \n");
  printf("흰빛이 많이 도는 핑크나 라벤더 컬러의 블러셔를 볼 중앙에 여리여리하게 발색하면 청순감을 더할 수 있습니다. \n");
  printf("3. 입술 화장 \n");
  printf("딸기우유를 연상하게 하는 핑크 컬러를 입술에 전체적으로 채워 발라주세요. 그리고 글로시한 체형의 틴트가 잘 어울립니다. \n");
  printf("4. 헤어 \n");
  printf("진한 흑갈색이나 애쉬 브라운 컬러의 염색은 투명한 피부를 강조해 줍니다. 그리고 내추럴한 긴 생머리가 특유의 청초한 느낌을 살려 주며 잘 어울립니다. \n");
	break;

case (result == summer cool mute):
	printf("%s 님께 어울리는 색상은 다음과 같습니다. \n", name);
	printf(" 더스티 로즈, 더스티 블루, 카메오 핑크, 오키드 스모크 등의 색이 있습니다. \n");
	printf("%s 님께 어울리는 코디는 다음과 같습니다. \n", name);
	printf("회기가 도는 빛 바랜 색감이 잘 어울리고, 탁색을 입었을 때 성숙한 특유의 느낌이 살아나며 차분한 무드를 느낄 수 있어요. 소프트한 컬러들이 베스트입니다. 새하얀 화이트, 옐로브라운 보다 크림색, 적갈색을 사용해주세요. 그리고 블랙&화이트처럼 큰 대비감은 소화하기 어려워요. 중간보다 살짝 더 약한 대비감이 좋고, 부드러운 먹색이나 차콜같은 컬러를 추천드립니다. 미색, 고동색같은 뉴트럴 컬러에 뮤트 컬러를 더하면 베스트 색감으로 연출할 수 있어요. 보색 매치는 피해주세요. 같은 색상 계열을 사용한 톤온톤으로 배색을 해보세요. \n");
  printf("옷의 소재는 깨끗한 면이나 셔츠 소화도가 높고, 광택이 심하지 않은 실크, 쉬폰 소재가 잘 어울려요. 보들보들하고 매트한 느낌의 니트도 무난히 가능합니다. 에스닉한 무드의 재질, 패턴, 스타일은 모두 피해주세요. 너무 러블리한 레이스나 볼륨감있는 니트처럼 질감이 도드라지는 옷은 고급스러운 에너지를 분산시켜요. 또한 애니메이션 무드도 소화하기 어렵습니다. \n");
	printf("%s 님께 어울리는 메이크업 스타일은 다음과 같습니다. \n", name);
	printf("1. 눈 화장 \n");
  printf("회색이나 탁한 핑크 컬러의 음영 쉐도우를 단독으로 바르는 것이 가장 잘 어울립니다. 블랙 컬러의 아이라인을 이용하되, 눈화장은 최소한으로 하는 것이 좋습니다. \n");
  printf("2. 치크 화장 \n");
  printf("톤 다운된 핑크 컬러의 블러셔를 사선 방향으로 바르되, 색이 너무 강조되지 않게 바르는 것이 어울립니다. \n");
  printf("3. 입술 화장 \n");
  printf("말린 장미 컬러의 매트한 립스틱은 차분하고 지적인 분위기를 더해 줍니다. \n");
  printf("4. 헤어 \n");
  printf("브라운, 애쉬 계열의 염색 등 너무 어둡거나 밝지 않은 색이라면 모두 잘 어울립니다. 어깨를 조금 넘는 생머리에 드라이로 볼륨을 넣는 자연스러운 스타일링이 청순한 느낌을 줍니다. \n");
	break;

case (result == warm mute autumn):
	printf("%s 님께 어울리는 색상은 다음과 같습니다. \n", name);
	printf("오트밀, 아이보리, 인디코랄, 올리브, 밝은 카기 등의 색이 있습니다. \n");
	printf("%s 님께 어울리는 코디는 다음과 같습니다. \n", name);
	printf("대비감이 어울리지 않기 때문에 부드러운 컬러나 비슷한 컬러를 사용해 통일감을 주고, 다양한 밝기의 컬러를 시도할 때에는 중명도의 컬러를 함께 사용하면 좋습니다. 과한 스타일링보다는 꾸안꾸 무드가 잘 어울립니다. \n");
  printf("옷의 소재는 하늘하늘하고 부드러운 소재도 좋고 거칠고 광택감 없는 린넨도 좋습니다. \n");
	printf("%s 님께 어울리는 메이크업 스타일은 다음과 같습니다. \n", name);
  printf("1. 눈 화장 \n");
  printf("과한 스모키보다는 적당하고 자연스러운 음영 메이크업이 잘 어울립니다. \n");
  printf("2. 치크 화장 \n");
  printf("베이지와 브라운이 섞인 듯한 아주 자연스럽고 부드러운 색을 추천합니다. \n");
	printf("3. 입술 화장 \n");
  printf("채도 낮은 코랄핑크, 무화과, 톤 다운된 베이지, 말린 장미가 잘 어울립니다. \n");
  printf("4. 헤어 \n");
  printf("다크 브라운, 내츄럴 브라운, 레드 브라운 등 부드럽고 살짝 탁한 기운이 있는 브라운 컬러가 좋습니다. \n");
	break;

case (result == autumn warm deep):
	printf("%s 님께 어울리는 색상은 다음과 같습니다. \n", name);
	printf("짙은 레드나 버건디, 카키색이나 짙은 초록색, 짙은 브라운 등의 색이 있습니다.\n");
	printf("%s 님께 어울리는 코디는 다음과 같습니다. \n", name);
	printf("옐로 베이스의 선명하고 깊은 색감이 잘 어울려요. 진한 컬러를 고급스럽고 편안하게 소화합니다. 페일톤이나 탁색, 웟낵은 유치한 느낌이 들 수 있어요. 저명도, 중-고채도의 컬러가 좋습니다. 블랙보다는 다크브라운, 새하얀 화이트보다는 오트밀 컬러를 추천합니다. \n");
  printf("그리고 대비감이 꼭 필요한 타입이에요. 인위적인 느낌 자체를 소화하지 못하기 때문에 형광색, 차가운 색은 다 튕겨내버려요. 중후하고 무게감 있는 컬러를 편안히 소화하는 것이 특징입니다. 딥한 컬러 안에서 톤온톤 배색, 보색 배색 모두 잘 어울려요. 성숙하고 고급스러운 무드를 보여줄 수 있습니다.\n");
  printf("옷의 재질은 에스닉하고 앤틱한 무드를 자연스레 사용할 수 있습니다. 큰 무늬와 무거운 재질감도 편안히 소화합니다. 빳빳한 셔츠, 면, 린넨, 니트, 스웨이드나 레더를 추천드려요. 그러나 인공적인 느낌의 애미메이션 그래픽, 여리여리한 무드의 쉬폰 레이스는 살짝 떠보이는 느낌이 들어 연출하기 어려울 수 있어요. \n");
	printf("액세서리는 실버보다는 골드나 로즈골드 색상을 선택하는 것이 좋아요. \n");
  printf("%s 님께 어울리는 메이크업 스타일은 다음과 같습니다. \n", name);
	printf("1. 눈 화장 \n");
  printf("노란빛이 도는 브라운 섀도우를 베이스로 깔고, 잿빛이 도는 브라운이나 카키색 섀도우를 활용하여 깊은 음영을 주세요. 짙은 갈색 아이라이너로 선명해 보이는 인상을 만들어주세요. \n");
  printf("2. 치크 화장 \n");
  printf("오렌지 브라운 컬러의 블러셔를 사선 방향으로 발색해주세요. 섀딩과 자연스럽게 연결하면 건강하고 관능적인 느낌을 줍니다. \n");
  printf("3. 입술 화장 \n");
  printf("아이 메이크업이 강조될 수 있도록, 스킨 톤의 립스틱을 발라주세요. 차분함과 고급스러움을 더해 줍니다. \n");
  printf("4. 헤어 \n");
  printf("붉은 빛이 도는 딥한 오렌지 브라운 컬러의 염색이 잘 어울립니다. 그리고 굵은 웨이브가 들어간 긴 머리는 무게감 있으면서 안정된 분위기를 연출해줍니다. \n");
	break;

case (result == winter cool bright):
	printf("%s 님께 어울리는 색상은 다음과 같습니다. \n", name);
	printf(" 레몬 옐로우, 딥 레드, 블루베이스의 코랄, 오키드 등의 색이 있습니다.\n");
	printf("%s 님께 어울리는 코디는 다음과 같습니다. \n", name);
	printf("고명도, 고채도의 컬러가 좋습니다. 특히 채도가 중요합니다. 탁색이나 파스텔 계열의 컬러를 입으면 존재감이 흐려진 듯 얼굴만 둥둥 떠다디는 느낌을 받을 수 있으니 주의하세요. \n");
  printf("또한 대비감이 반드시 필요한 타입입니다. 상의와 하의이 대비가 아니어도 괜찮습니다. 액세서리나 검은 머리카락, 붉은 립 등 다양한 포인트 디테일로 대비감을 줄 수 있습니다. 대비감이 적을수록 얼굴이 칙칙해보이니 조심해주세요. \n");
  printf("옷의 소재는 빳빳하고 무늬와 질감이 없는 소재, 패딩 소재, 약간의 실크 광택까지 무리없이 소화합니다. 스트라이프나 큰 글씨의 프린팅도 추천드립니다. 깨끗함을 보존해야 하기 때문에 트위드나 주름이 잡힌 옷은 코디의 마이너스 요소가 될 수 있어요. 잔무늬와 재질감이 느껴지는 옷보다는 민무늬가 좋습니다. \n");
  printf("액세서리는 골드보다는 실버가 좋고, 큰 보석으롱 장식된 액세서리를 하는 것이 잘 어울려요. \n");
	printf("%s 님께 어울리는 메이크업 스타일은 다음과 같습니다. \n", name);
	printf("1. 눈 화장 \n");
  printf("은색 펄이 든 베이지 컬러의 섀도우를 발라주세요. 그리고 블랙 컬러로 아이라인을 날렵하고 깔끔하게 그려주세요. 그리고 인조 속눈썹이나 마스카라를 활용하여 속눈썹을 강조해 주세요. \n");
  printf("2. 치크 화장 \n");
  printf("흰빛이 도느 핑크나 라벤더 컬러의 블러셔를 볼 중앙에 컬러감만 비칠 정도로 연하게 발색해주세요. \n");
  printf("3. 입술 화장 \n");
  printf("푸른빛이 도는 쨍한 핑크 컬러의 립스틱을 입술 전체에 포인트가 될 수 있도록 발라주세요. \n");
  printf("4. 헤어 \n");
  printf("노란빛이 적고 딥한 브라운 또는 와인 계열로 염색하는 것이 잘 어울립니다. 그리고 가슴까지 오는 길이에 물결 웨이브를 넣어 화려하고 고급스러운 분위기를 표현할 수 있습니다. \n");
	break;

case (result == winter cool deep):
	printf("%s 님께 어울리는 색상은 다음과 같습니다. \n", name);
	printf("체리 핑크, 차콜, 블랙, 화이트 등의 색이 있습니다. \n");
	printf("%s 님께 어울리는 코디는 다음과 같습니다. \n", name);
	printf("블랙이 제일 잘 어울리는 타입으로 무채색, 올 블랙을 고급스럽고 세련되게 소화합니다. 횟기가 될거나 따뜻한 컬러는 연출이 어려워요. 차가운 색상을 입었을 때 피부색이 정돈되며 겨울 특유의 깨끗함을 배로 느낄 수 있어요. \n");
  printf("그리고 대비감이 반드시 필요한 타입입니다. 블랙&화이트가 가장 잘 어울리며, 두 색을 함께 사용해 선명한 이미지를 살려주는 것이 중요합니다. 중후하고 무게감 있는 컬러를 편안히 소화할 수 있고, 성숙하고 고고한 무드를 보여줄 수 있습니다. \n");
  printf("옷의 소재는 빳빳한 시스루, 에나멜, 실크, 스팽글, 벨벳 소재를 추천드립니다. 여리여리한 무드의 쉬폰 레이스는 상반된 에너지로 분위기 있게 연출하기 어려울 수 있어요. 러블리한 꽃무늬나 애니메이션 프린팅보다는 하운드투스 체크, 애니멀 패턴, 스트라이프를 사용할 때 더 세련되고 선명한 느낌을 줍니다. \n);
  printf("액세서리는 골드보다는 실버가 좋고, 큰 보석으로 장식된 액세서리를 하는 것이 잘 어울려요. \n");
	printf("%s 님께 어울리는 메이크업 스타일은 다음과 같습니다. \n", name);
	printf("1. 눈 화장 \n");
  printf("은색 펄이 든 베이지 컬러의 섀도우를 발라주세요. 그리고 블랙 컬러의 아이라인을 날렵하고 깔끔하게 그려주세요. 또한 인조 속눈썹이나 마스카라를 활용하여 속눈썹을 강조해주세요. \n");
  printf("2. 치크 화장 \n");
  printf("붉은 기가 올라오기 때문에 블러셔를 사용하지 않는 것이 깔끔합니다. 사용할 경우 연한 라벤더 컬러의 블러셔를 사선 방향으로 약하게 발색하는 것이 어울립니다. \n");
  printf("3. 입술 화장 \n");
  printf("톤 다운된 와인색이나 딥한 버건디 컬러의 립스틱이 잘 어울립니다. \n");
  printf("4. 헤어 \n");
  printf("푸른 빛이 도는 흑발 또는 딥한 브라운 계열로 염색할 경우 흰 피부를 더욱 돋보이게 해 줍니다. 귀를 살짝 덮는 길이의 숏컷으로 도도한 이미지를 연출할 수 있습니다. \n");
	break;
}
